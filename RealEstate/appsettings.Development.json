{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "RealEstate": "<YOUR_SQL_CONNECTION_STRING>"
  },

  "MongoDB": {
    "ConnectionString": "<YOUR_MONGODB_CONNECTION_STRING>",
    "Database": "RealEstateDb"
  },

  "Jwt": {
    "Issuer": "RECAM-API",
    "Audience": "localhost3000",
    "Key": "<YOUR_JWT_SECRET_KEY>"
  },

  "FrontendUrl": "https://reamp-brady-ltudsfaa7-depeng-zhangs-projects.vercel.app",
  "AllowedHosts": "*",

  "EmailSettings": {
    "SmtpServer": "<YOUR_SMTP_SERVER>",
    "Port": 587,
    "SenderName": "RealEstateApp",
    "SenderEmail": "<YOUR_EMAIL_ADDRESS>",
    "Username": "<YOUR_EMAIL_USERNAME>",
    "Password": "<YOUR_EMAIL_PASSWORD>",
    "EnableSsl": true
  },

  "AzureBlobStorage": {
    "ConnectionString": "<YOUR_AZURE_BLOB_CONNECTION_STRING>",
    "ContainerName": "depengre"
  }
